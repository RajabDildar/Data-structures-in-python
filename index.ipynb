   "source": [
    "def fast_exponentiation(a,b):\n",
    "    ans = 1\n",
    "    while b>0:\n",
    "        if b&1 != 0:\n",
    "            ans = ans*a\n",
    "        else:\n",
    "            a = a * a\n",
    "        b=b//2\n",
    "    return ans\n",
    "\n",
    "fast_exponentiation(5,4)"
   ]
  }
